@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix omg: <https://w3id.org/omg#> .
@prefix fog: <https://w3id.org/fog#> .
@prefix bot: <https://w3id.org/bot#> .
@prefix gom: <https://w3id.org/gom#> .
@prefix ex:  <https://example.org/demo#> .

# 坐标系 —— 注意这里用 gom:CartesianCoordinateSystem
ex:LocalCS a gom:CartesianCoordinateSystem .

# 业务元素 —— 关联几何
ex:road1 a bot:Element ;
  omg:hasGeometry ex:g1 .

# 几何对象 —— 关联坐标系（gom:hasCoordinateSystem），并用带“点号”的 OBJ 属性名
ex:g1 a omg:Geometry ;
  gom:hasCoordinateSystem ex:LocalCS ;
  fog:asObj_v3.0-obj "https://raw.githubusercontent.com/Liuyh2433209777/road/main/demo/road.obj"^^xsd:anyURI .